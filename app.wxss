/**app.wxss**/
@import './utils/iconfont.wxss';
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
page{
  font-size: 28rpx;
}
image{
  display: block;
}

button {
  border: 0px;
  border-radius: 0px;
  background: none;
  margin: 0px;
  padding: 0px;
}

button::after {
  border: 0px;
  border-radius: 0px;
  background: none;
  margin: 0px;
  padding: 0px;
}

button[disabled] {
  background: none;
}
